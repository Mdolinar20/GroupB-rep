@{
    ViewData["Title"] = "Index";
}

<h3>Upcomming Events</h3>
<hr />

@if (ViewBag.EventList != null)
{
    <table class="table table-hover">
        <tr>
            <th>Event title</th>
            <th>Organizer</th>
            <th>Description</th>
            <th>Start Time</th>
            <th>End Time</th>
        </tr>
        
        @foreach (var item in ViewBag.EventList as List<GroupB_Project.Models.CalenderEvent>)
        {
    <tr>
        <td>@item.Summery</td>
        <td>@item.Organizer</td>
        <td>@item.Description</td>
        <td>@item.StartTime</td>
        <td>@item.EndTime</td>
    </tr>
        }
    </table>
}
else
{
    <div class="alert alert-danger">No Upcomming events</div>
}